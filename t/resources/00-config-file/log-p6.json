{
	"writers": [
		{
			"type": "custom",
			"require": "LogP6::WriterConf::Journald",
			"fqn-method": "Factory::EXPORT::DEFAULT::&writer-conf-journald",
			"args": {
				"name": "journald",
				"pattern": "%level %msg",
				"auto-exceptions": true,
				"use-priority": true,
				"use-code-file": true,
				"use-code-line": true,
				"use-code-func": true,
				"use-mdc": true,
				"systemd": {
					"type": "custom",
					"require": "Custom",
					"fqn-class": "Custom::TestSystemd"
				}
			}
		}
	],
	"filters": [
		{
			"type": "std",
			"name": "journald",
			"level": "trace"
		}
	],
	"cliches": [
		{
			"name": "journald",
			"matcher": "journald",
			"grooves": [ "journald", "journald" ]
		}
	]
}